{
  "name": "back",
  "version": "1.0.0",
  "description": "API REST avec Sequelize et MySQL",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all"
  },
  "keywords": [
    "API",
    "REST",
    "Sequelize",
    "MySQL"
  ],
  "author": "Olivier Brun",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.11",
    "npm": "^11.6.2",
    "sequelize-cli": "^6.6.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "sequelize": "^6.37.7"
  }
}
